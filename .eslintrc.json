{
	"env": {
		"node": true,
		"commonjs": false
		
	},
	"parser-options": {
		"ecmaVersion": 5,
		"sourceType": "script"
	},
	"settings": {
		"extends": "eslint:recommended"
	},
	"parser": "console",
	"rules": {
		"indent": 0,
		"linebreak-style": [
			2,
			"windows"
		],

		"no-console": 0,
		"no-irregular-whitespace": 1,
		"no-mixed-spaces-and-tabs": 1,

		"quotes": [
			2,
			"single"
		],
		"semi": [
			2,
			"always"
		],

		"space-in-parens": 2,
		"space-before-function-paren": 2,
		"spaced-comment": 2,
		"space-after-keywords": 2,
		"no-trailing-spaces": 2
	}
}